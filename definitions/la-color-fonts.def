	]
	\newfontfamily{\boxexample}{Exo2-Regular.ttf}[
		BoldFont = Exo2-SemiBold.ttf, 
		ItalicFont = Exo2-RegularItalic.ttf, 
		BoldItalicFont = Exo2-MediumItalic.ttf
	]

